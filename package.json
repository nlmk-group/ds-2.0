  "scripts": {
    "check-types": "tsc --noemit",
    "convert:dark-theme-tokens": "node script/creatingDarkThemeConst.js",
    "start": "yarn convert:dark-theme-tokens && vite",
    "lint": "eslint ./src && yarn check-types",
    "clean": "rm -rf lib && rm -rf cache",
    "snowpack:build": "rm -rf lib && yarn convert:dark-theme-tokens && snowpack build --polyfill-node",
    "build": "./script/build",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir lib/dist/components",
    "packing": "yarn build && npm publish",
    "packing:beta": "yarn build && npm publish --tag beta",
    "conveyor": "yarn snowpack:build",
    "deploy": "yarn conveyor && yarn packing",
    "test": "jest",
    "test:clear-cache": "jest --clearCache",
    "test:ci": "jest --watchAll=false --ci --config=jest.config.js",
    "test:coverage": "jest  --coverage",
    "test:generate-output": "jest --json --env=jsdom --outputFile=.jest-test-results.json || true",
    "prebuild:storybook": "yarn test:generate-output",
    "storybook": "yarn convert:dark-theme-tokens && yarn prebuild:storybook && storybook dev -p 6006",
    "storybook:dev": "yarn convert:dark-theme-tokens  && storybook dev -p 6006",
    "storybook:build": "yarn convert:dark-theme-tokens && rm -rf storybook-static && yarn prebuild:storybook && storybook build && cp -r dist/* storybook-static",
    "serve": "yarn storybook:build && npx serve storybook-static",
    "storybook:build:windows": "yarn convert:dark-theme-tokens && (if exist storybook-static ( rd /s /q storybook-static)) && yarn prebuild:storybook && storybook build && xcopy /s /y dist storybook-static",
    "token:generate": "export $(cat .env | xargs) && node generateTokens.mjs",
    "token:generate:windows": "(for /F \"tokens=*\" %i in (.env) do set %i) && node generateTokens.mjs",
    "commit": "cz",
    "packpackage": "cd lib && yarn pack",
    "generateIconsExports": "ts-node src/components/Icon/generateIconsExports.ts  && prettier --write src/components/Icon",
    "chromatic": "yarn storybook:build && npx chromatic --project-token=chpt_aea96cf2943eaa5 --exit-zero-on-changes -d storybook-static/",
    "chromatic:windows": "yarn storybook:build:windows && npx chromatic --project-token=chpt_aea96cf2943eaa5 --exit-zero-on-changes -d storybook-static/"
  },
  "pre-commit": [
    "lint"
  ],
